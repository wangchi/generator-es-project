{
  "name": "generator-es-project",
  "version": "0.0.3",
  "description": "Generate a ES Next project with react, or vue, or no frameworks, select your like",
  "homepage": "https://github.com/wangchi/generator-es-project",
  "author": {
    "name": "Wang Chi",
    "email": "hiwangchi@gmail.com",
    "url": "https://github.com/wangchi"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "ES6",
    "ES Next",
    "React",
    "Vue",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "prettier": "^1.15.2",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-config-xo": "^0.25.0",
    "jest": "^23.5.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "wangchi/generator-es-project",
  "license": "MIT"
}
